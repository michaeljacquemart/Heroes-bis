    "axios": "^0.19.0",
    "node-sass": "^4.12.0",
    "parcel-bundler": "^1.12.3",
    "serialize-to-js": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
